{
  "manifest_version": 3,
  "name": "YAFFeather",
  "version": "3.8.1",
  "description": "Yet Another Fork of Feather. Originally by sw33ze, tweaked by Alex.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [{
    "matches": [
      "https://*.nationstates.net/*"
    ],
    "js": [
      "content.js"
    ]
  }],

  "options_ui":{
    "page":"settings.html"
  },

  "permissions":["storage"]
}
