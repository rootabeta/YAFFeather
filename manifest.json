{
  "manifest_version": 2,
  "name": "YAFFeather Beta",
  "version": "3.6.7.2",
  "description": "BETA for YAFFeather; Yet Another Fork of Feather. Originally by sw33ze, tweaked by Alex.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [{
    "matches": [
      "https://*.nationstates.net/*"
    ],
    "js": [
      "content.js"
    ]
  }],

  "options_ui":{
    "page":"settings.html"
  },

  "permissions":["storage"],

  "browser_specific_settings":{
	  "gecko":{
		  "id":"yaffeatherbeta@alexfromnationstates"
	  }
  }
}
